{
  "name": "manage-v2-frontend",
  "private": true,
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/loyaltycorp/manage-v2-frontend.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --nohoist=*",
    "bootstrap:ci": "lerna bootstrap --ci --force-local",
    "build": "node scripts/exec.js build",
    "build:ci": "node scripts/exec.js build:ci",
    "dev": "node scripts/exec.js dev",
    "clean": "lerna clean --yes && yarn && yarn bootstrap",
    "cli": "node scripts/cli.js",
    "commit": "npx git-cz",
    "publish-packages": "lerna publish --yes --registry=\"https://npm.pkg.github.com/\"",
    "lint": "eslint --ext .js,.vue --fix . && stylelint --fix \"packages/**/*.vue\" \"packages/**/*.scss\" && prettier --list-different --write \"**/*.{json,css,scss,md}\" && markdownlint *.md",
    "list-package": "lerna ls -l",
    "unit": "vue-cli-service test:unit",
    "unit:watch": "yarn unit --watch --notify --notifyMode change"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@babel/runtime-corejs2": "^7.3.4",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@vue/cli-plugin-babel": "3.6.0",
    "@vue/cli-plugin-e2e-cypress": "3.6.0",
    "@vue/cli-plugin-eslint": "3.6.0",
    "@vue/cli-plugin-pwa": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "3.6.3",
    "@vue/cli-service": "3.6.0",
    "@vue/eslint-config-prettier": "4.0.1",
    "@vue/eslint-config-standard": "4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "^9.4.10",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "24.7.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "commitizen": "^3.0.5",
    "cross-env": "5.2.0",
    "cz-lerna-changelog": "^2.0.2",
    "deepmerge": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-vue": "5.2.2",
    "express": "4.17.3",
    "fs-extra": "^7.0.1",
    "husky": "^1.3.1",
    "hygen": "2.1.2",
    "imagemin-lint-staged": "0.4.0",
    "inquirer": "^6.2.2",
    "jest": "^24.5.0",
    "jest-canvas-mock": "^2.0.0-beta.1",
    "jest-junit": "^6.3.0",
    "jest-mock-axios": "^2.4.0",
    "lerna": "^3.16.4",
    "lint-staged": "^8.1.5",
    "markdownlint-cli": "0.15.0",
    "merge-deep": "^3.0.2",
    "node-sass": "4.12.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "sass-loader": "7.1.0",
    "stylelint": "9.10.1",
    "stylelint-config-css-modules": "1.4.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recess-order": "2.0.2",
    "stylelint-config-standard": "18.3.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "3.6.0",
    "tailwindcss": "^0.7.4",
    "tailwindcss-plugins": "^0.1.3",
    "vue": "^2.6.10",
    "vue-eslint-parser": "^6.0.3",
    "vue-jest": "^3.0.4",
    "vue-router": "^3.0.2",
    "vue-svg-loader": "^0.12.0",
    "vue-template-compiler": "2.6.10",
    "vuepress": "0.14.11",
    "vuex": "^3.1.0",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-subresource-integrity": "^1.3.2",
    "yargs": "^13.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "cross-env PRE_COMMIT=true lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  },
  "publishConfig": {
    "access": "restricted"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "md5": "^2.2.1"
  }
}
